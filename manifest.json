{
  "name": "Practran",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Fast keyworld translating by single click based on Google Translate",
  "content_scripts": [{
    "js": [
      "config/config.js",
      "bower_components/requirejs/require.js",
      "config/patch.js",
      "main.js",
      "config/init.js"
    ],
    "css": [
      "dialog/dialog.style.css"
    ],
    "matches": [ "http://*/*", "https://*/*"]
  }],
  "web_accessible_resources": [
    "bower_components/*",
    "dialog/*",
    "apis/*"
  ]
}